--LoadingMessageก๊อปไปใช้ได้
game.StarterGui:SetCore("SendNotification",  {
 Title = "MITUMA X HUB";
 Text = "Loading UI - If nothing happen that mean not support map.";
 Icon = "";
 Duration = 10;
 Callback = NotificationBindable;
})
--เช็คแมพ
wait(8) do
local gameid = game.PlaceId

if(gameid == 4747388345) then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/MITUMADev/op/main/KramlifeFreeScript.lua", true))()

elseif(gameid == 192800) then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/MITUMADev/PaidScript/main/WorkAtPizzaPlacePaid", true))()

end
end
--หมัดไฟก๊อปไปใช้ได้


local Fire = Instance.new("Fire")
    Fire.Parent = game.Players.LocalPlayer.Character.LeftHand
    Fire.Heat = 5
    Fire.Size = 4
    local Fire = Instance.new("Fire")
    Fire.Parent = game.Players.LocalPlayer.Character.RightHand
    Fire.Heat = 5
    Fire.Size = 4

